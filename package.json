{
  "name": "eye-oh-see",
  "version": "0.3.3",
  "description": "A TypeScript IoC library",
  "keywords": [
    "TS",
    "typescript",
    "IoC",
    "DI",
    "inversion",
    "control",
    "dependency",
    "injection"
  ],
  "main": "dist/src/Index.js",
  "typings": "dist/src/Index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint ./**/*.ts",
    "pretest": "npm run lint",
    "test": "mocha --require ts-node/register test/**/*.ts --recursive",
    "test:watch": "npm run test -- --watch",
    "prebuild": "npm run clean && npm run test",
    "build": "tsc -p .",
    "precommit": "npm run build"
  },
  "license": "MIT",
  "devDependencies": {
    "chai": "3.5.0",
    "husky": "0.11.6",
    "mocha": "2.5.3",
    "rimraf": "2.5.4",
    "ts-node": "1.2.1",
    "typescript": "1.8.10",
    "typings": "1.3.2"
  },
  "dependencies": {
    "reflect-metadata": "0.1.3"
  }
}
